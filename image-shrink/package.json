{
  "name": "image-shrink",
  "productName": "ImageShrink",
  "version": "1.0.0",
  "description": "App to optimize images for website",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ImageShrink\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=false --out=release-builds"
  },
  "author": "Kentaro Kawamura",
  "license": "ISC",
  "devDependencies": {
    "electron": "^11.0.3",
    "electron-packager": "^15.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-svgo": "^8.0.0",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "electron-log": "^4.3.0",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "slash": "^3.0.0"
  }
}
